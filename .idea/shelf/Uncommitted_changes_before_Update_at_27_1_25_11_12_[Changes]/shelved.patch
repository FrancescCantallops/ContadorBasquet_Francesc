<+><!DOCTYPE html>\n<html lang=\"es\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Marcador de Bàsquet</title>\n  <link rel=\"stylesheet\" type=\"text/css\" href=\"styles.css\">\n  <script src=\"funcions.js\"></script>\n  <script src=\"contador.js\"></script>\n</head>\n<body>\n\n<div class=\"container mt-5\">\n\n  <!-- Cronómetro -->\n  <div>\n    <div>\n      <h2 class=\"title text-center\" id=\"timer\">10:00</h2>\n      <button class=\"score-button\" onclick=\"startCountdown()\">Inicia Contador</button>\n      <button class=\"score-button\" onclick=\"resetCountdown()\">Reiniciar Contador</button>\n      <button class=\"score-button\" onclick=\"pauseCountdown()\">Pausa</button>\n      <button class=\"score-button\" onclick=\"resumeCountdown()\">Reanudar</button>\n    </div>\n  </div>\n</div>\n\n\n<div class=\"container\">\n  <header class=\"title text-center\">Marcador de Bàsquet</header>\n  <nav class=\"links\">Enllaços a altres pàgines</nav>\n  <div class=\"period-container text-center\">\n    <div id=\"period\" class=\"period-number\">1</div>\n  </div>\n  <table class=\"scoreboard\">\n    <tbody>\n    <tr>\n      <td>Local</td>\n      <td>Visitant</td>\n    </tr>\n    <tr>\n      <td id=\"points-a\">0</td>\n      <td id=\"points-b\">0</td>\n    </tr>\n    <tr>\n      <td id=\"fouls-a\">0</td>\n      <td id=\"fouls-b\">0</td>\n    </tr>\n    <tr>\n      <td>\n        <button class=\"score-button\" id=\"canviar_puntuacio_local_1\" onclick=\"canviar_puntuacio(1, 'local')\">+1</button>\n        <button class=\"score-button\" id=\"canviar_puntuacio_local_2\" onclick=\"canviar_puntuacio(2, 'local')\">+2</button>\n        <button class=\"score-button\" id=\"canviar_puntuacio_local_3\" onclick=\"canviar_puntuacio(3, 'local')\">+3</button>\n        <button class=\"score-button\" id=\"canviar_faltes_local\" onclick=\"canviar_faltes(1, 'local')\">Falta</button>\n      </td>\n      <td>\n        <button class=\"score-button\" id=\"canviar_puntuacio_visitant_1\" onclick=\"canviar_puntuacio(1, 'visitant')\">+1</button>\n        <button class=\"score-button\" id=\"canviar_puntuacio_visitant_2\" onclick=\"canviar_puntuacio(2, 'visitant')\">+2</button>\n        <button class=\"score-button\" id=\"canviar_puntuacio_visitant_3\" onclick=\"canviar_puntuacio(3, 'visitant')\">+3</button>\n        <button class=\"score-button\" id=\"canviar_faltes_visitant\" onclick=\"canviar_faltes(1, 'visitant')\">Falta</button>\n      </td>\n    </tr>\n    </tbody>\n  </table>\n  <div class=\"gestor\">\n    <td><button class=\"score-button\" id=\"reset\" onclick=\"iniciar_partida()\">Iniciar partida</button></td>\n    <td><button class=\"score-button\" id=\"canviPeriode\" onclick =\"canviar_periode()\">Canvi de període</button></td>\n    <td><button class=\"score-button\" id=\"canviar_mode\" onclick=\"alternar_mode()\">Restar / sumar</button></td>\n  </div>\n  <div class=\"history\">\n    <h2>Historial de la Partida</h2>\n    <div id=\"historial\">\n      <p> Inici del partit </p>\n    </div>\n\n  </div>\n</div>\n</body>\n</html>\n\n\n\n
